{
  "name": "@majoyal/react-modal-image",
  "version": "2.1.5",
  "description": "Lightweight Lightbox React Component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "site": "gh-pages -d demo/dist",
